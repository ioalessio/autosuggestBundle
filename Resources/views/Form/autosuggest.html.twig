{% block autosuggest_widget %}
{% spaceless %}
    {{ form_widget(form.autosuggest, { 'attr' : { 'class': 'ajax-typeahead ', 'data-value': form.value.vars['id'], 'data-link' : form.vars['attr']['url'] } } ) }}
    {{ form_widget(form.value) }}
    {{ form_rest(form) }}
{% endspaceless %}
{% endblock autosuggest_widget %}